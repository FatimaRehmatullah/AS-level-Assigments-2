Dim newstr As String = ""
Dim s As String = ""
Dim LocN As Integer = 0
Dim LocB As integer = 0

Console.Write("Enter string: ")
s = Console.Readline

LocN = Instr(s,"not")
LocB = Instr(s,"bad")

  If LocB > LocN Then
   For Count = 1 to LocN-1
     newstr = newstr & MID(s,count,1)
   Next
    
newstr = newstr & "good"
 
    For Count =LocN+3 to LEN(s)
     newstr = newstr & MID(s,count,1)
   Next
  Endif
  
   Console.Write(newstr)
   
   Console.Readkey
